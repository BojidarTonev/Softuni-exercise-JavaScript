
<div class="movie">
    <h2>Title: {{ title }}</h2>
    <img
        src="{{ imageUrl }}">
    <p>Available Tickets: {{ tickets }}</p>
    <div class="btn-group">
        <button><a href="#/movies/edit-movie/{{_id}}">Edit</a></button>
        <button><a href="#/movies/delete-movie/{{_id}}">Delete</a></button>
        <button><a href="#/movies/buy-movie/{{_id}}">Buy Ticket</a></button>
        <button><a href="#/movies/details-movie/{{_id}}">Details</a></button>
    </div>
</div>
