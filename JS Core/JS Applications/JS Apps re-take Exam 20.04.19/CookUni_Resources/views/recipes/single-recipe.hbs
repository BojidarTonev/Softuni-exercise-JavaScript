<div class="col-lg-12">
    <div class="our-team-main">

        <div class="team-front">
            <img src="https://cdn.pixabay.com/photo/2014/12/11/02/55/corn-syrup-563796__340.jpg" />
            <h3>{{meal}}</h3>
            <p>{{category}}</p>
        </div>

        <div class="team-back">
            <div class="back-side-info">
                <h4>Ingredients</h4>
                <ul>
                    {{#each ingredients}}
                        <li>{{>ingredient}}</li>
                    {{/each}}
                </ul>
                <a href="/#/recipes/details/{{_id}}">View the recepie</a>
            </div>

            <img class="foodImage" src="{{foodImageURL}}" width:
                150; height: auto; />
        </div>

    </div>
</div>